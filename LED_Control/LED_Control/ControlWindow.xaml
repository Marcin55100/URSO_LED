<Window x:Class="LED_Control.ControlWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LED_Control"
        mc:Ignorable="d"
        Title="LED Control" Height="350" Width="525">
    <Grid>
        <Label x:Name="Infolabel" Content="" HorizontalAlignment="Center"  VerticalAlignment="Center"/>
        <StackPanel Name="mainPanel" Orientation="Vertical" Width="50" HorizontalAlignment="Center">
            <StackPanel.Resources>
                <Style TargetType="{x:Type Button}">
                    <Setter Property="Margin" Value="0,10,0,0"/>
                </Style>
            </StackPanel.Resources>
            <!--<Button x:Name="ONbutton" Content="ON" HorizontalAlignment="Center"  VerticalAlignment="Center" Width="75" Click="ONbutton_Click"/>
        <Button x:Name="OFFbutton" Content="OFF" HorizontalAlignment="Center" VerticalAlignment="Center" Width="75" Click="OFFbutton_Click"/>
             
            -->
           
            </StackPanel>
        
    </Grid>
</Window>
